{
  "hosting": {
    "site": "murmurs",
    "public": "out",
    "ignore": [
      "**/.*",
      "**/node_modules/**",
      "**/*.env*"
    ],
    "redirects": [],
    "rewrites": [
      {
        "source": "/login",
        "destination": "/login.html"
      },
      {
        "source": "/main",
        "destination": "/main.html"
      },
      {
        "source": "!/static/**",
        "destination": "/index.html"
      },
      {
        "source": "!/_next/**",
        "destination": "/index.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
